<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/theme_background">

    <com.example.hlp.myui.TitlieMyUI
        android:id="@+id/title_office_manage"
        xmlns:myui="http://schemas.android.com/apk/res/com.example.hlp.activitytest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        myui:title="办公管理">
    </com.example.hlp.myui.TitlieMyUI>

    <GridView
        android:layout_below="@+id/title_office_manage"
        android:id="@+id/id_gv"
        android:numColumns="3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </GridView>
    <RelativeLayout
        android:layout_margin="10dp"
        android:layout_below="@+id/id_gv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/bt_my_apply"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的申请(0)"/>
        <Button
            android:layout_alignParentRight="true"
            android:id="@+id/bt_my_shenpi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的审批(0)"/>
    </RelativeLayout>

</RelativeLayout>