<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.example.hlp.myui.TitlieMyUI
        xmlns:myui="http://schemas.android.com/apk/res/com.example.hlp.activitytest"
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        myui:title="配件申请"
        myui:editVisibility="false"
        myui:addVisibility="true">
    </com.example.hlp.myui.TitlieMyUI>
    <LinearLayout
        android:layout_below="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <com.example.hlp.myui.Item01MyUI
            xmlns:myui="http://schemas.android.com/apk/res/com.example.hlp.activitytest"
            android:id="@+id/device_manage"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            myui:icon="@drawable/sbgl_icon"
            myui:text1="待审批">
        </com.example.hlp.myui.Item01MyUI>
        <com.example.hlp.myui.Item01MyUI
            xmlns:myui="http://schemas.android.com/apk/res/com.example.hlp.activitytest"
            android:id="@+id/office_manage"
            android:layout_marginTop="1dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            myui:icon="@drawable/bggl_icon"
            myui:text1="已通过">
        </com.example.hlp.myui.Item01MyUI>
        <com.example.hlp.myui.Item01MyUI
            xmlns:myui="http://schemas.android.com/apk/res/com.example.hlp.activitytest"
            android:id="@+id/my_msg"
            android:layout_marginTop="1dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            myui:icon="@drawable/wdxx_icon"
            myui:text1="已驳回">
        </com.example.hlp.myui.Item01MyUI>
        <com.example.hlp.myui.Item01MyUI
            xmlns:myui="http://schemas.android.com/apk/res/com.example.hlp.activitytest"
            android:id="@+id/about_us"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            myui:icon="@drawable/wdsb_icon"
            myui:text1="给我抄送">
        </com.example.hlp.myui.Item01MyUI>
    </LinearLayout>
</RelativeLayout>